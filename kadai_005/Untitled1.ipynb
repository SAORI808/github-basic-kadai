        "# 台形の各辺の長さを定義\n",
        "number1 = 10  # 上辺の長さ(cm)\n",
        "number2 = 20  # 下辺の長さ(cm)\n",
        "number3 = 5  # 高さ(cm)\n",
        "\n",
        "# 台形の面積の計算式\n",
        "number4 = (number1 + number2) * number3 / 2\n",
        "\n",
        "# 結果の出力\n",
        "print(f\"{number4}cm²\")"
      ]
